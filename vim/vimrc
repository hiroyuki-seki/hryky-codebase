"maximizes Window.
autocmd GUIEnter * simalt ~x 

nnoremap <C-s><C-s> :w<CR>
nnoremap <C-s><C-a> :mksession!<CR>
nnoremap <C-s><C-k> :bp<Bar>:bd<Space>#<CR>
nnoremap <F11> :cnext<CR>
set guioptions-=m
set guioptions-=T
set guioptions-=r
set guioptions-=R
set guioptions-=l
set guioptions-=L
set guioptions-=b
set grepprg=grep\ -n\ --exclude=tags\ --exclude=TAGS\ --exclude=XTAGS
set hlsearch
set iminsert=0 "disables Input Method in Insert mode.
set imsearch=0 "disables Input Method when entering a search parttern.
set list
set listchars=tab:>-,trail:-
set number
set autoindent
set smartindent
set statusline=%f\ (%l,%c)\ %y\ %m\ %r
set verbose=0
syntax on
set shiftwidth=4
set tabstop=4
set noexpandtab

let g:hryky={}
function hryky.StrDate(...)
	return strftime("%Y-%m-%d", 0 <# a:0 ? a:1 : localtime())
endfunction
function hryky.StrTime(...)
	return strftime("%H:%M:%S", 0 <# a:0 ? a:1 : localtime())
endfunction
function hryky.StrDateTime(...)
	let time = 0 <# a:0 ? a:1 : localtime()
	return g:hryky.StrDate(time) . "T" . g:hryky.StrTime(time)
endfunction
